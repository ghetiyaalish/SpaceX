<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Define the client action -->
    <record id="action_awesome_dashboard" model="ir.actions.client">
        <field name="name">Awesome Dashboard</field>
        <field name="tag">awesome_dashboard</field> 
    </record>

    <!-- Define the menu root -->
    <menuitem 
        id="menu_dashboard_root" 
        name="SpaceX Dashboards"
        sequence="10"
        />
    

    <!-- Define the menu item -->
    <menuitem 
        id="menu_awesome_dashboard" 
        name="Awesome Dashboard"
        parent="menu_dashboard_root"
        action="action_awesome_dashboard"
        sequence="10"/>
        
</odoo>